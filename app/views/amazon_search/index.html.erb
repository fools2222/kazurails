<header class="shadow-sm bg-dark text-white ">
  <h1 class="p-4">kazutter</h1>
  <p class="p-3">スクレイピング　結果</p>
</header>

<main class="mt-3">
    <div class="container mt-5">
        <table class="table">
            <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">物件名</th>
                <th scope="col">住所</th>
                <th scope="col">金額</th>
            </tr>
            </thead>
                <%@amazon.each do |item|%>
                <tbody>
                <tr>
                    <th scopw="row"><%=item.id%></th>
                    <td><%=item.title%></td>
                    <td><%=item.subject%></td>
                    <td><%=item.body%></td>
                    </tr>
                </tbody>
                <%end%>
        </table>
    </div>
</main>



<footer class="container rounded pt-3 pb-2 bg-light mt-3">
    <div class="justify-content-center text-center mt-5">
        <p>Copyright © 2022 Kazutter</p>
    </div>
</footer>

