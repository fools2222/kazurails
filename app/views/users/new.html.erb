<h1>sign up</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_with(model: @user,local: true) do |f| %>
      <%=f.label :name %>
      <%=f.text_field :name %>
      <%=f.label :email %>
      <%=f.email_field :email %>
      <%=f.submit "送信" %>
    <%end%>
  </div>
</div>